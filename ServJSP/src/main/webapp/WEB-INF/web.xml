<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  
    <!-- ********************************************************************************************************** --> 
  
  <servlet>
  <servlet-name>serv1</servlet-name>
  <servlet-class>com.adm.MyServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>serv1</servlet-name>
  <url-pattern>/serv</url-pattern>
  </servlet-mapping>
  
  <context-param>
  <param-name>name</param-name>
  <param-value>Abhinav</param-value>
  </context-param>
  
  <context-param>
  <param-name>phone</param-name>
  <param-value>Samsung</param-value>
  </context-param>
  <!--*********************************************************************************************************************--> 
  <servlet>
  <servlet-name>serv2</servlet-name>
  <servlet-class>com.adm.SqServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>serv2</servlet-name>
  <url-pattern>/sq</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>serv3</servlet-name>
  <servlet-class>com.adm.SumServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>serv3</servlet-name>
  <url-pattern>/sum</url-pattern>
  </servlet-mapping>
  
</web-app>